        "estree-walker": "^2.0.2",
        "less": "^4.1.2",
        "less-plugin-autoprefix": "^2.0.0",
        "less-plugin-clean-css": "^1.5.1",
        "lodash.find": "^4.6.0",
        "lodash.map": "^4.6.0",
        "lodash.sortby": "^4.7.0",
