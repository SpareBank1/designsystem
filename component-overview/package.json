        "classnames": "^2.3.1",
        "estree-walker": "^2.0.2",
        "less": "^4.1.2",
        "less-plugin-autoprefix": "^2.0.0",
        "less-plugin-clean-css": "^1.5.1",
        "lodash.find": "^4.6.0",
        "lodash.map": "^4.6.0",
        "lodash.sortby": "^4.7.0",
        "npm-run-all": "^4.1.5",
        "parcel": "^2.4.0",
        "path-browserify": "^1.0.1",
        "process": "^0.11.10",
        "react-docgen": "^3.0.0",
        "rollup": "^2.70.1"
    }
