        "@sb1/ffe-core": "^26.0.0",
        "@sb1/ffe-core-react": "^6.0.1",
        "@sb1/ffe-datepicker": "^10.0.45",
        "@sb1/ffe-datepicker-react": "^5.2.15",
        "@sb1/ffe-decorators-react": "^3.0.10",
        "@sb1/ffe-details-list-react": "^3.0.77",
        "@sb1/ffe-dropdown-react": "^5.1.15",
        "@sb1/ffe-file-upload": "^7.0.16",
        "@sb1/ffe-file-upload-react": "^6.1.5",
        "@sb1/ffe-form": "^22.0.0",
        "@sb1/ffe-form-react": "^10.0.2",
