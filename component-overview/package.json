{
    "name": "@sb1/ffe-component-index",
    "version": "4.2.9",
    "description": "Index of FFE compontents",
    "license": "MIT",
    "author": "SpareBank 1 Utvikling",
    "main": "./lib/index.js",
    "module": "./lib/esm/index.js",
    "targets": {
        "main": false,
        "module": false
    },
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:SpareBank1/designsystem.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "alias": {
        "buffer": false
    },
    "@parcel/transformer-css": {
        "drafts": {
            "nesting": true,
            "customMedia": true
        }
    },
    "scripts": {
        "build": "make all",
        "start": "parcel webapp/index.html --no-cache --no-autoinstall --no-source-maps --open",
        "watch": "run-p watch:es watch:less watch:examples",
        "watch:es": "chokidar \"node_modules/@sb1/ffe-*/es/**/*.js\" --follow-symlinks -c \"touch package.json\"",
        "watch:less": "chokidar \"node_modules/@sb1/ffe-*/less/**/*.less\" --follow-symlinks -c \"make -B lib/style.css\"",
        "watch:examples": "chokidar \"examples/**/*.jsx\" -c \"make lib/index.js\"",
        "ghpages": "make -B build ghpages",
        "clean": "make clean"
    },
    "dependencies": {
        "@sb1/ffe-accordion": "^9.0.4",
        "@sb1/ffe-accordion-react": "^10.0.7",
        "@sb1/ffe-account-selector-react": "^21.3.5",
        "@sb1/ffe-buttons": "^17.0.3",
        "@sb1/ffe-buttons-react": "^18.0.2",
        "@sb1/ffe-cards": "^15.0.3",
        "@sb1/ffe-cards-react": "^11.0.2",
        "@sb1/ffe-chart-donut-react": "^5.2.0",
        "@sb1/ffe-collapse-react": "^2.0.3",
        "@sb1/ffe-context-message": "^7.1.4",
        "@sb1/ffe-context-message-react": "^8.0.8",
        "@sb1/ffe-core": "^27.0.1",
        "@sb1/ffe-core-react": "^7.1.1",
        "@sb1/ffe-datepicker": "^11.0.6",
        "@sb1/ffe-datepicker-react": "^6.0.8",
        "@sb1/ffe-decorators-react": "^3.0.12",
        "@sb1/ffe-details-list-react": "^4.0.5",
        "@sb1/ffe-dropdown-react": "^6.0.6",
        "@sb1/ffe-file-upload": "^8.0.6",
        "@sb1/ffe-file-upload-react": "^7.0.9",
        "@sb1/ffe-form": "^23.0.6",
        "@sb1/ffe-form-react": "^11.0.9",
        "@sb1/ffe-formatters": "^4.0.11",
        "@sb1/ffe-grid": "^14.0.4",
        "@sb1/ffe-grid-react": "^13.0.5",
        "@sb1/ffe-header": "^19.0.2",
        "@sb1/ffe-icons": "^12.17.1",
        "@sb1/ffe-icons-react": "^7.3.5",
        "@sb1/ffe-lists": "^12.0.4",
        "@sb1/ffe-lists-react": "^6.0.6",
        "@sb1/ffe-message-box": "^11.0.4",
        "@sb1/ffe-message-box-react": "^8.0.7",
        "@sb1/ffe-sb1-logos": "^1.1.3",
        "@sb1/ffe-searchable-dropdown-react": "^15.1.5",
        "@sb1/ffe-spinner": "^5.0.4",
        "@sb1/ffe-spinner-react": "^6.0.5",
        "@sb1/ffe-system-message": "^7.0.4",
