{
    "packages": ["packages/*", "linting/*", "buildtool", "component-overview"],
    "npmClient": "npm",
    "version": "100.4.0",
    "command": {
        "publish": {
            "allowBranch": "main",
            "conventionalCommits": true,
            "ignoreChanges": [
                ".eslintrc",
                ".gitattributes",
                ".gitignore",
                ".npmignore",
                ".npmrc",
                "*.md",
                "*.spec.js",
                "test-setup.js",
                "ffe-header/images/logo-*.svg"
            ],
            "message": "chore(release): publish"
        }
    }
}
