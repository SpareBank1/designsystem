{
    "name": "@sb1/ffe-symbols-react",
    "version": "7.0.11",
    "description": "React-komponenter for Material Symbols tilpasset Sb1 sine behov",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "type": "module",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --clean",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "test": "ffe-buildtool jest",
        "test:watch": "ffe-buildtool jest --watch"
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:SpareBank1/designsystem.git"
    },
    "author": "SpareBank1",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SpareBank1/designsystem/issues"
    },
    "homepage": "https://github.com/SpareBank1/designsystem#readme",
    "dependencies": {
        "@sb1/ffe-symbols": "^4.0.16",
        "classnames": "^2.3.2",
        "react": "^18.2.0"
    },
    "devDependencies": {
        "@sb1/ffe-buildtool": "^0.10.0",
        "eslint": "^9.22.0",
        "tsup": "^8.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
