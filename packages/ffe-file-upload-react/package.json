{
    "name": "@sb1/ffe-file-upload-react",
    "version": "12.0.74",
    "description": "Upload file button with validation and list of uploaded files.",
    "keywords": [
        "ffe"
    ],
    "license": "MIT",
    "author": "SpareBank 1",
    "files": [
        "lib",
        "types"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "type": "module",
    "types": "types/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "require": "./lib/index.cjs"
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:SpareBank1/designsystem.git"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --clean",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "test": "ffe-buildtool jest",
        "test:watch": "ffe-buildtool jest --watch"
    },
    "dependencies": {
        "@sb1/ffe-buttons-react": "^24.3.7",
        "@sb1/ffe-file-upload": "^12.0.28",
        "@sb1/ffe-form": "^32.0.25",
        "@sb1/ffe-icons-react": "^12.0.1",
        "classnames": "^2.3.1"
    },
    "devDependencies": {
        "@sb1/ffe-buildtool": "^0.10.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0"
    },
    "peerDependencies": {
        "react": ">=18.2.0"
    },
    "publishConfig": {
