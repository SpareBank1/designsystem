{
    "name": "@sb1/ffe-icons-react",
    "version": "12.0.1",
    "description": "React-komponenter for ffe-icons",
    "license": "MIT",
    "author": "SpareBank 1",
    "files": [
        "lib",
        "types",
        "src"
    ],
    "main": "./lib/index.cjs",
    "types": "types/index.d.ts",
    "module": "./lib/index.js",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "require": "./lib/index.cjs"
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:SpareBank1/designsystem.git"
    },
    "scripts": {
        "build": "tsup src/index.tsx --format esm,cjs --clean && tsc --emitDeclarationOnly",
        "clean": "rimraf gen-src lib es types",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "test": "ffe-buildtool jest"
    },
    "dependencies": {
        "@sb1/ffe-icons": "^17.0.2"
    },
    "devDependencies": {
        "@sb1/ffe-buildtool": "^0.10.0",
        "eslint": "^9.22.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0"
    },
    "peerDependencies": {
        "react": ">=18.2.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
