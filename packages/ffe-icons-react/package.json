{
  "name": "@sb1/ffe-icons-react",
  "version": "7.0.3",
  "description": "React-komponenter for ffe-icons",
  "license": "MIT",
  "files": [
    "lib",
    "es"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "es/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:SpareBank1/designsystem.git"
  },
  "scripts": {
    "build": "npm run build:tsx-components && npm run build:cjs && npm run build:es",
    "build:cjs": "tsc tsx/index.ts --outDir lib --module commonjs --jsx react --declaration",
    "build:es": "tsc tsx/index.ts --outDir es --module esnext --jsx react --declaration --skipLibCheck",
    "build:tsx-components": "node ./src/build-tsx-components.js",
    "clean": "rimraf tmp tsx lib es",
    "lint": "eslint src/.",
    "test": "npm run lint"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@sb1/ffe-icons": "^12.11.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.0",
    "json-loader": "^0.5.7",
    "mkdirp": "^0.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
