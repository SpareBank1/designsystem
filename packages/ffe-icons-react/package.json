{
  "name": "@sb1/ffe-icons-react",
  "version": "7.0.0",
  "description": "React-komponenter for ffe-icons",
  "license": "MIT",
  "files": [
    "lib",
    "es"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:SpareBank1/designsystem.git"
  },
  "scripts": {
    "build": "npm run build:jsx-components && npm run build:cjs && npm run build:es",
    "build:cjs": "babel -d lib --env-name=cjs --root-mode=upward jsx",
    "build:es": "babel -d es --env-name=es --root-mode=upward jsx",
    "build:jsx-components": "node ./src/build-jsx-components.js",
    "clean": "rimraf tmp jsx lib es",
    "lint": "eslint src/.",
    "test": "npm run lint"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@sb1/ffe-icons": "^12.9.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.0",
    "json-loader": "^0.5.7",
    "mkdirp": "^0.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
