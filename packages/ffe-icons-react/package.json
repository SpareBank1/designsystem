    "json-loader": "^0.5.7",
    "mkdirp": "^0.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
