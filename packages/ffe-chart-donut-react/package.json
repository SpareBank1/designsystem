{
    "name": "@sb1/ffe-chart-donut-react",
    "version": "9.0.5",
    "description": "Donut chart in SVG",
    "keywords": [
        "ffe"
    ],
    "license": "MIT",
    "author": "SpareBank 1",
    "files": [
        "lib",
        "less",
        "css",
        "types"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "types": "./types/index.d.ts",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./lib/index.js",
            "require": "./lib/index.cjs"
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:SpareBank1/designsystem.git"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --clean && tsc --emitDeclarationOnly && lessc --include-path=../../node_modules less/ffe-chart-donut.less css/ffe-chart-donut.css --autoprefix",
        "lint": "eslint --config ./eslint.config.js \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint --config ./eslint.config.js \"src/**/*.{ts,tsx}\" --fix",
        "test": "ffe-buildtool jest",
        "test:watch": "ffe-buildtool jest --watch"
    },
    "devDependencies": {
        "@sb1/ffe-buildtool": "^0.10.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0"
    },
    "peerDependencies": {
        "react": ">=18.2.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
