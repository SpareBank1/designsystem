{
  "name": "@sb1/ffe-account-selector-react",
  "version": "18.2.9",
  "description": "Selector for bank accounts with autocomplete.",
  "keywords": [
    "ffe"
  ],
  "license": "MIT",
  "author": "SpareBank 1",
  "files": [
    "lib",
    "es",
    "less",
    "types"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:SpareBank1/designsystem.git"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:types",
    "build:cjs": "babel -d lib/. --root-mode=upward  --env-name=cjs --ignore=*.spec.js src/.",
    "build:es": "babel -d es/. --root-mode=upward --env-name=es --ignore=*.spec.js src/.",
    "build:types": "copyfiles -f src/index.d.ts types/",
    "lint": "eslint src/.",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "../../test-setup.js"
  },
  "dependencies": {
    "@sb1/ffe-form-react": "^8.2.0",
    "@sb1/ffe-formatters": "^4.0.1",
    "@sb1/ffe-icons-react": "^7.2.19",
    "@sb1/ffe-searchable-dropdown-react": "^13.3.9",
    "@sb1/ffe-spinner-react": "^5.1.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react-auto-bind": "^0.4.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-window": "^1.8.5"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "jest": "^26.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "peerDependencies": {
    "@sb1/ffe-form": "*",
    "@sb1/ffe-spinner": "*",
    "react": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
