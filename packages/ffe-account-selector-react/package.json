{
    "name": "@sb1/ffe-account-selector-react",
    "version": "31.2.15",
    "description": "Selector for bank accounts with autocomplete.",
    "keywords": [
        "ffe"
    ],
    "license": "MIT",
    "author": "SpareBank 1",
    "files": [
        "lib",
        "less",
        "css",
        "types"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "type": "module",
    "types": "types/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "require": "./lib/index.cjs"
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:SpareBank1/designsystem.git"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --clean && lessc --include-path=../../node_modules less/ffe-account-selector.less css/ffe-account-selector.css --autoprefix",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "test": "ffe-buildtool jest",
        "test:watch": "ffe-buildtool jest --watch"
    },
    "dependencies": {
        "@sb1/ffe-formatters": "^5.0.10",
        "@sb1/ffe-icons-react": "^12.0.1",
        "@sb1/ffe-searchable-dropdown-react": "^23.1.31",
        "classnames": "^2.3.1",
        "react-custom-scrollbars-4": "^4.5.1"
    },
    "devDependencies": {
        "@sb1/ffe-buildtool": "^0.10.0",
        "eslint": "^9.22.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0"
    },
    "peerDependencies": {
        "react": ">=18.2.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
