{
    "name": "@sb1/ffe-searchable-dropdown-react",
    "version": "23.1.31",
    "description": "Dropdown with search option",
    "license": "MIT",
    "author": "SpareBank 1",
    "files": [
        "lib",
        "less",
        "css",
        "types"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "types": "types/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "require": "./lib/index.cjs"
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:SpareBank1/designsystem.git"
    },
    "scripts": {
        "build": "tsup src/index.tsx --format esm,cjs --clean && tsc --emitDeclarationOnly && lessc --include-path=../../node_modules less/searchable-dropdown.less css/searchable-dropdown.css --autoprefix",
        "lint": "eslint \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "test": "ffe-buildtool jest",
        "test:watch": "ffe-buildtool jest --watch"
    },
    "dependencies": {
        "@sb1/ffe-chips-react": "^2.0.26",
        "@sb1/ffe-core-react": "^10.1.2",
        "@sb1/ffe-form": "^32.0.25",
        "@sb1/ffe-icons-react": "^12.0.1",
        "@sb1/ffe-spinner-react": "^10.0.20",
        "@types/lodash.isequal": "^4.5.8",
        "classnames": "^2.3.1",
        "compute-scroll-into-view": "^3.1.0",
        "lodash.debounce": "^4.0.8",
        "lodash.isequal": "^4.5.0",
        "react-custom-scrollbars-2": "^4.3.0"
    },
    "devDependencies": {
        "@sb1/ffe-buildtool": "^0.10.0",
        "eslint": "^9.22.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0"
    },
    "peerDependencies": {
        "react": ">=18.2.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
