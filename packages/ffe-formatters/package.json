{
  "name": "@sb1/ffe-formatters",
  "version": "4.0.1",
  "description": "JavaScript general use string formatters",
  "keywords": [
    "ffe"
  ],
  "license": "MIT",
  "author": "SpareBank 1",
  "files": [
    "lib",
    "es",
    "types"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:SpareBank1/designsystem.git"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:es && npm run build:types",
    "build:cjs": "babel -d lib/. --root-mode=upward  --env-name=cjs --ignore=*.spec.js src/.",
    "build:es": "babel -d es/. --root-mode=upward --env-name=es --ignore=*.spec.js src/.",
    "build:types": "copyfiles -f src/index.d.ts types/",
    "lint": "eslint src/.",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "jest": "^26.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
