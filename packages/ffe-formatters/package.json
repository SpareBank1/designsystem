{
    "name": "@sb1/ffe-formatters",
    "version": "5.0.10",
    "description": "JavaScript general use string formatters",
    "keywords": [
        "ffe"
    ],
    "license": "MIT",
    "author": "SpareBank 1",
    "files": [
        "lib",
        "types"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "type": "module",
    "types": "types/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "require": "./lib/index.cjs"
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:SpareBank1/designsystem.git"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --clean",
        "lint": "eslint --config ./eslint.config.js \"src/**/*.{ts,tsx}\"",
        "lint:fix": "eslint --config ./eslint.config.js \"src/**/*.{ts,tsx}\" --fix",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@sb1/ffe-buildtool": "^0.10.0",
        "eslint": "^9.22.0",
        "tsup": "^8.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
